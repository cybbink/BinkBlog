<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bink.dao.DictKeywordMapper">
    <!--根据博客id查询字典名称-->
    <select id="getDictNameByKid" resultType="String">
        SELECT keyword from d_keyword where kid=#{kid}
    </select>
</mapper>