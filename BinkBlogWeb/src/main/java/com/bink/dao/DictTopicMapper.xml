<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bink.dao.DictTopicMapper">
    <!--根据博客id查询字典名称-->
    <select id="getDictNameByTopicId" resultType="String">
        SELECT keyword from d_topic where topic_id=#{topicId}
    </select>
</mapper>